                    </sheet>

                </form>
            </field>
        </record>

        <!-- TREEVIEW -->

        <record id="view_competitiveness_management_commercial_name_tree" model="ir.ui.view">
            <field name="name">competitiveness_management.commercial_name.tree</field>
